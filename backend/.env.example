# Klaew Klad Backend Environment Variables

# Application
ENVIRONMENT=development
DEBUG=true
API_V1_STR=/api/v1

# Database
DATABASE_URL=sqlite+aiosqlite:///./klaew_klad.db

# CORS
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]

# AI Models (optional - for Huawei Cloud integration)
# HUAWEI_CLOUD_PROJECT_ID=your_project_id
# HUAWEI_CLOUD_ACCESS_KEY=your_access_key
# HUAWEI_CLOUD_SECRET_KEY=your_secret_key

# Sentinel Hub (optional - for real satellite data)
# SENTINEL_HUB_CLIENT_ID=your_client_id
# SENTINEL_HUB_CLIENT_SECRET=your_secret
